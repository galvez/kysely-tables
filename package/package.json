  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/json-diff": "^1.0.3",
    "@types/node": "^18.0.0",
    "@types/pg": "^8.15.1",
    "fastify": "^5.3.3",
    "oxlint": "^0.16.10",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4"
