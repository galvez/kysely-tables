  "author": "Jonas Galvez <jonasgalvez@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^0.10.1",
    "@types/minimist": "^1.2.5",
    "better-sqlite3": "^11.10.0",
    "deep-object-diff": "^1.1.9",
    "fast-safe-stringify": "^2.1.1",
    "json-diff": "^1.0.6",
    "kysely": "^0.28.2",
    "minimist": "^1.2.8",
    "pg": "^8.15.6",
    "picocolors": "^1.1.1",
    "postgrator": "^8.0.0",
    "scule": "^1.3.0",
    "typescript": "^5.0.0",
    "ufo": "^1.6.1"
